#divSesionUsuario {
	position: fixed;
	right: 10px;
	top: 14px;
	font-family: Century Gothic, sans-serif;
}

#divSesionUsuario>span {
	color: white;
	font-family: Century Gothic, sans-serif;
}

#top {
	background-color: #0063AA;
	box-shadow: 0px 2px 2px rgba(0, 0, 0, 0.8);
	height: 105px;
	left: 0px;
	position: fixed;
	top: 0px;
	width: 100%;
	z-index: 1000;
	font-family: Century Gothic, sans-serif;
}

.center_content {
	position: relative;
	background-color: white;
	box-shadow: 0px 0px 2px black;
	padding: 5px;
	margin-top: 115px;
	font-family: Century Gothic, sans-serif;
}

#bottom {
	position: relative;
	background-color: #009A3C;
	padding: 5px;
	margin: 10px 0px 0px 0px;
	text-align: center;
	font-weight: bold;
	color: white;
	font-family: Century Gothic, sans-serif;
}


