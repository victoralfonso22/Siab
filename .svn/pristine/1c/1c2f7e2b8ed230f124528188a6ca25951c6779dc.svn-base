@ManagedBean
@ApplicationScoped
public class RutasAplicacion {

    /**
     * Creates a new instance of inicialBean
     */
    public RutasAplicacion() {
    }
    
    public String getBaseUrl()
    {
        return Utilidades.urlBase();
    }
    
